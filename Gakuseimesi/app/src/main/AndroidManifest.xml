<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.makotokobayashi.gakuseimesi">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.SubActivity2"></activity>

        <activity
            android:name=".activity.main.GenreListActivity"
            android:label="@string/title_activity_genre_list"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"></activity>

        <activity android:name=".activity.storeInfomation.StoreInfomationActivity"></activity>
    </application>

</manifest>